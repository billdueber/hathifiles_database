$:.unshift "../lib"

require 'hathifiles_database'

filename = ARGV[0]
connection = HathifilesDatabase::Database.new('mysql2://localhost/hf');

connection.update_from_file filename

#dump_file_paths = connection.start_from_scratch(filename, destination_dir: '/tmp/ddd')
#dump_file_paths.values.each {|x| Pathname.new(x).delete }

